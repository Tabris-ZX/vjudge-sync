{
  "manifest_version": 3,
  "name": "VJudge-Sync",
  "version": "1.0.0-beta",
  "description": "VJudge 一键同步归档已绑定的oj过题记录,目前支持洛谷,牛客,cf,atc,qoj,uoj",
  "author": "Tabris_ZX",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "cookies"
  ],
  "host_permissions": [
    "*://vjudge.net/*",
    "*://*.vjudge.net/*",
    "*://*.luogu.com.cn/*",
    "*://*.nowcoder.com/*",
    "*://*.codeforces.com/*",
    "*://*.kenkoooo.com/*",
    "*://*.qoj.ac/*",
    "*://*.uoj.ac/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "32": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
